<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:paddingLeft="5dp"
              android:paddingRight="5dp"
              android:layout_height="match_parent">
    <com.github.ksoichiro.android.observablescrollview.ObservableScrollView
        android:scrollbars="none"
        android:id="@+id/myresume_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:background="@color/background"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:background="@drawable/customborder"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/myresume_user_image"
                    android:layout_width="100dp"
                    android:layout_height="100dp"/>
                <LinearLayout
                    android:paddingTop="3dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Name: "
                            android:textColor="@color/Black"
                            android:textSize="@dimen/activity_title_medium"
                            android:textStyle="bold"/>
                        <TextView
                            android:id="@+id/myresume_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="abc"
                            android:textSize="@dimen/activity_content_big"
                            android:textColor="@color/Black"/>
                    </LinearLayout>
                    <LinearLayout
                        android:paddingTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Sex: "
                            android:textColor="@color/Black"
                            android:textSize="@dimen/activity_title_small"
                            android:textStyle="bold"/>
                        <TextView
                            android:id="@+id/myresume_sex"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="abc"
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"/>
                    </LinearLayout>
                    <LinearLayout
                        android:paddingTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Hometown: "
                            android:textColor="@color/Black"
                            android:textSize="@dimen/activity_title_small"
                            android:textStyle="bold"/>
                        <TextView
                            android:id="@+id/myresume_hometown"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="abc"
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"/>
                    </LinearLayout>
                    <LinearLayout
                        android:paddingTop="3dp"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Birthday: "
                            android:textColor="@color/Black"
                            android:textSize="@dimen/activity_title_small"
                            android:textStyle="bold"/>
                        <TextView
                            android:id="@+id/myresume_birthday"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="abc"
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:background="@color/background"
                android:layout_width="match_parent"
                android:layout_height="3dp"/>

            <LinearLayout
                android:orientation="vertical"
                android:background="@drawable/customborder"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:paddingLeft="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:weightSum="1"
                    android:orientation="horizontal">
                    <ImageView
                        android:layout_gravity="center_vertical"
                        android:layout_width="25dp"
                        android:background="@drawable/resumecontacticon"
                        android:layout_height="25dp"/>
                    <TextView
                        android:textStyle="bold"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/activity_title_medium"
                        android:paddingLeft="5dp"
                        android:text="Contact"
                        android:gravity="center_vertical"
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"/>
