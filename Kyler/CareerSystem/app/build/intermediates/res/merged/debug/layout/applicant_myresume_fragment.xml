                            android:id="@+id/myresume_phone"
                            android:paddingLeft="10dp"
                            android:text="abc "
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </TableRow>
                    <TableRow
                        android:paddingTop="3dp"
                        android:paddingLeft="20dp">
                        <TextView
                            android:text="Email "
                            android:textSize="@dimen/activity_title_small"
                            android:textColor="@color/Black"
                            android:textStyle="bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/myresume_email"
                            android:paddingLeft="10dp"
                            android:text="abc "
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </TableRow>
                    <TableRow
                        android:paddingTop="3dp"
                        android:paddingLeft="20dp">
                        <TextView
                            android:text="Address "
                            android:textSize="@dimen/activity_title_small"
                            android:textColor="@color/Black"
                            android:textStyle="bold"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:id="@+id/myresume_address"
                            android:paddingLeft="10dp"
                            android:paddingRight="70dp"
                            android:text="abc "
                            android:textSize="@dimen/activity_content_medium"
                            android:textColor="@color/Black"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </TableRow>
                </TableLayout>
            </LinearLayout>

            <TextView
