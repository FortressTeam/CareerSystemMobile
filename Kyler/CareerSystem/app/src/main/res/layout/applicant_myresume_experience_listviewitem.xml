<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/myresume_experience_listviewitem_conpanyname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="abc"
        android:textColor="@color/Black"
        android:textStyle="bold"
        android:textSize="@dimen/activity_title_small"/>
    <TextView
        android:id="@+id/myresume_experience_listviewitem_position"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="abc"
        android:textColor="@color/Black"
        android:textSize="@dimen/activity_content_medium"/>
    <TextView
        android:id="@+id/myresume_experience_listviewitem_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="abc"
        android:textColor="@color/Black"
        android:textSize="@dimen/activity_content_medium"/>
    <TextView
        android:id="@+id/myresume_experience_listviewitem_description"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="abc"
        android:textColor="@color/Black"
        android:textSize="@dimen/activity_content_medium"/>
</LinearLayout>
